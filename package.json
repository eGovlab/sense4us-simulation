{
  "name": "template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/init"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "connect": "^3.3.4",
    "ejs": "latest",
    "ejs-locals": "latest",
    "express": "~4.9.0",
    "iconv-lite": "^0.4.5",
    "node-sass-middleware": "latest",
    "browserify": "latest",
    "immutable": "latest",
    "uglify-js": "latest",
    "watchify": "latest",
    "rh_config-parser": "latest",
    "rh_cookie-cutter": "latest",
    "rh_controller-layer": "latest",
    "rh_logger": "latest",
    "rh_router": "latest"
  },

  "author": "Robin Swenson, Anton Haughey, Jona Ekenberg",

  "scripts": {
    "prestart": "npm install; echo '\n * Running browserify for production!\n'; npm run browserify",
    "start": "node bin/init.js",
    "browserify": "node_modules/browserify/bin/cmd.js frontend/public/js/main.js | node_modules/uglify-js/bin/uglifyjs > frontend/public/js/bundle.js",
    "watch": "node_modules/watchify/bin/cmd.js frontend/public/js/main.js -o frontend/public/js/bundle.js --debug --verbose",
    "compile": "node_modules/watchify/bin/cmd.js frontend/public/js/main.js -o \"node_modules/uglify-js/bin/uglifyjs -cm > frontend/public/static/js/model-builder.min.js\" --verbose"
  }
}
